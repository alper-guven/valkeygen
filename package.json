{
  "name": "valkeygen",
  "version": "1.0.1",
  "description": "",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "test": "ts-mocha test/*.ts",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:both": "npm run build:esm && npm run build:cjs",
    "build": "npm run lint && rimraf ./dist && npm run build:both",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --exec node --loader ts-node/esm src/index.ts",
    "preversion": "npm run test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alper-guven/valkeygen.git"
  },
  "author": "Alper GÃ¼ven <alper.guven.11@gmail.com> (https://alper.codes/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alper-guven/valkeygen/issues"
  },
  "keywords": [
    "valkey",
    "valkey key",
    "valkey key generator",
    "valkey key creator",
    "create valkey key",
    "create valkey keys"
  ],
  "homepage": "https://github.com/alper-guven/valkeygen#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.19.69",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "chai": "^4.3.6",
    "eslint": "^8.22.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}